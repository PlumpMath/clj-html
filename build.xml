<project name="clj-html" default="jar">
  <property name="jarfile" location="clj-html.jar"/>

  <target name="clean">
    <delete file="${jarfile}"/>
  </target>

  <target name="jar">
    <jar jarfile="${jarfile}">
      <path location="LICENSE"/>
      <fileset dir="./src" includes="clj_html/**/*.clj"/>
    </jar>
  </target>
</project>
